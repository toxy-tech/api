{
    "name": "toxy-tech/api",
    "description": "API package for ToxyTech CMS",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "toxytech",
        "toxytech cms",
        "toxytech platform",
        "toxytech api"
    ],
    "homepage": "https://toxytech.com",
    "authors": [
        {
            "name": "Adham Elsharkawy",
            "email": "adhamshrqawy@icloud.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ToxyTech\\Api\\": "src"
        }
    },
    "require": {
        "google/auth": "^1.28"
    },
    "require-dev": {
        "knuckleswtf/scribe": "^5.0",
        "laravel/pint": "^1.5.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ToxyTech\\Api\\Providers\\ApiServiceProvider"
            ],
            "aliases": {
                "ApiHelper": "ToxyTech\\Api\\Facades\\ApiHelper"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
